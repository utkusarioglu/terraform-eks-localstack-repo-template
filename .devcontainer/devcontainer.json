{
  "name": "terraform-eks-localstack-repo-template.templates.utkusarioglu.com",
  "dockerComposeFile": [
    "../.docker/docker-compose.dev.yml",
    "docker-compose.yml"
  ],
  "runServices": [
    "localstack"
  ],
  "postCreateCommand": "scripts/post-create-command.sh",
  "service": "terraform",
  "workspaceFolder": "/utkusarioglu-com/templates/terraform-eks-localstack-repo-template",
  "customizations": {
    "vscode": {
      "extensions": [
        "golang.go",
        "hashicorp.terraform",
        "ms-kubernetes-tools.vscode-kubernetes-tools",
        "amazonwebservices.aws-toolkit-vscode"
      ],
      "settings": {
        "workbench.colorCustomizations": {
          "activityBar.background": "#2f78bd"
        }
      }
    }
  }
}
